#include<stdio.h>

int main(){
	int num, result=0, i, j;
	
	printf("Insira um numero: ");
	scanf("%d", &num);//pegando o numero para o loop
	
	for(i=0;i<num;i++){
		printf("\n");
		result++;//aqui ocorre a incrementação
		for(j=0;j<i+1;j++){
			printf("%d", result);//printando o resultado
		}
	}
	
	/*se colocar o result dentro do segundo loop que é o loop do j
	o valor de result incrementa todo de uma vez so*/
	
	return 0;
}
